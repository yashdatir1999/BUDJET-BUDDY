<%- include("code/header") %>
<link rel="stylesheet" href="/stylesheets/profile.css">

<div class="profile">
    <%- include("code/nav") %>
    <div class="nav">
            <h3><a href="/profilesetting"><%= admin.username%></a></h3>
        <form action="/search" method="post">
            <input type="text" name="search" placeholder="Search" id="">
        </form>
        <h3>Total Expances: <span><%= total%></span></h3>
        <a href="/addexpenses">ADD EXPENSES</a>
    </div>
    <div class="allexpenses">

        <% user.forEach(function(u , i){ %>
            <div class="expense">
                <div class="srno">
                    <h1># EXPENSE <%= i+1%></h1>
                </div>
                <h3>NAME : <%= u.expansesname%></h3>
                <h3>AMOUNT : â‚¹<%= u.expenseamount%></h3>
                <h3>CATAGORY : <%= u.catagory%></h3>
                <h3>TAG : <%= u.subcatagory%></h3>
                <div class="btns">
                    <a class="update" href="/updateexpenses/<%= u._id%>">Update</a>
                    <a class="delete" href="/deleteexpenses/<%= u._id%>">Delete</a>
                </div>
                <h6>Last Update: <input type="datetime" name="" value="<%= u.updatedAt%>" id=""></h6>
            </div>
        <%})%>

    </div>
</div>

<%- include("code/footer") %>